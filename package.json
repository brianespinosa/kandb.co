    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@bjeco/prettier-config": "*",
    "@types/node": "22.13.14",
    "@types/react": "19.0.12",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "lint-staged": "15.5.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "pretty-quick": "4.2.2",
    "sass": "1.86.0",
    "simple-git-hooks": "2.13.1",
